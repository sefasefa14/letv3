const Discord = require('discord.js');
const data = require('quick.db');
const ms = require('ms');

exports.run = async (client, message, args) => {
const datas = await data.fetch(`${message.author.id}.zaman.gs`);
if(Date.now() < datas) return message.channel.send(new Discord.MessageEmbed().setColor('#00001').setDescription(`${message.author} **__6__ saat de bir kullanabilirsiniz!**`)).then(m => m.delete({timeout: 6000}));
data.set(`${message.author.id}.zaman.gs`, Date.now()+ms('6h'));
message.channel.send(new Discord.MessageEmbed()
.setDescription(`${message.author} **Ã–zelden __SatÄ±ÅŸ sunucunu__ gÃ¶nderdim bakar mÄ±sÄ±n.**`));
message.author.send(`> **1 adet __SatÄ±ÅŸ__ sunucusu kurmak iÃ§in tema geldi.

> GÃ¼le gÃ¼le kullan ğŸ˜‡

https://discord.new/cvKnztQzsnPk`).catch(error => message.channel.send(new Discord.MessageEmbed().setDescription("MesajÄ± gÃ¶nderemedim. Muhtemelen DM'n kapalÄ±.")));
};
exports.conf = {
  enabled: true,
  guildOnly: true,
  aliases: [],
  permLevel: 0
}

exports.help = {
  name: 'tasarÄ±m-tema'
};